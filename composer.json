{
    "name": "shieldfy/shieldfy-php",
    "type": "library",
    "description": "Shieldfy Official PHP SDK - (shieldfy.io)",
    "keywords": [
        "shieldfy",
        "shieldfy-php",
        "waf",
        "firewall",
        "security"
    ],
    "homepage": "https://github.com/shieldfy/shieldfy-php",
    "license": "SCL",
    "authors": [
        {
            "name": "Shieldfy Inc.",
            "email": "team@shieldfy.io",
            "homepage": "https://shieldfy.io"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "ext-curl": "*",
        "ext-mbstring":"*",
        "ext-uopz":"*",
        "shieldfy/normalizer":"1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0||~6.0",
        "mikey179/vfsStream":"^1.6",
        "phpdocumentor/phpdocumentor":"2.*",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Shieldfy\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "src/Helper.php"
        ],
        "psr-4": {
            "Shieldfy\\": "tests"
        }
    },
    "bin":[
        "bin/Install"
    ],
    "config": {
        "sort-packages": true
    }
}
